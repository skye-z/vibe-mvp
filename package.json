{
  "name": "opencode-vibemvp",
  "version": "1.0.0",
  "description": "OpenCode plugin for MVP-style project management and documentation generation",
  "main": "dist/plugin.js",
  "type": "module",
  "files": [
    "dist",
    "template"
  ],
  "scripts": {
