  "repository": {
    "type": "git",
    "url": "https://github.com/skye-z/vibe-mvp"
  },
  "homepage": "https://github.com/skye-z/vibe-mvp",
  "bugs": {
    "url": "https://github.com/skye-z/vibe-mvp/issues"
  },
  "dependencies": {
    "@opencode-ai/plugin": "^1.1.28",
    "gray-matter": "^4.0.3",
    "vue": "^3.5.27"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitepress": "^1.3.0",
    "vitest": "^2.0.0"
  }
